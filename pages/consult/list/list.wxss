.container{
  height: 100%;
}

.search{
  padding: 25rpx 30rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.searchLeft{
  width: 590rpx;
  padding-left: 45rpx;
  height: 80rpx;
  display: flex;
  align-items: center;
  background-color: #FAFAFA;
  border-radius: 40rpx;
  color: #666666;
  font-size: 25rpx;
  border: 1rpx solid #E7E8E8;
}

.searchLeft view{
  margin-left: 20rpx;
}

.kefu > view{
  position: relative;
}

.kefu button{
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  opacity: 0;
}

.kefu image{
  width: 34rpx;
  height: 33rpx;
}

.kefu view{
  color: #666666;
  font-size: 18rpx;
}

.screen{
  height: 75rpx;
  display: flex;
}

.screen > view{
  width: 25%;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 29rpx;
  font-weight: bold;
}

.screen > view.true{
  color: #FFAB2C;
}

.screen > view image{
  width: 11rpx;
  height: 7rpx;
  margin-left: 8rpx;
  margin-top: 10rpx;
}

.noData{
  text-align: center;
  font-size: 27rpx;
  color: #818692;
  font-weight: bold;
  padding-top: 270rpx;
}

.noData image{
  width: 276rpx;
  height: 156rpx;
  margin-bottom: 60rpx;
}

.fenlei{
  position: fixed;
  left: 0;
  width: 100%;
  background-color: rgba(0, 0, 0, 0.5);
  z-index: 9;
  border-top: 1rpx solid #CCCCCC;
  display: flex;
  flex-direction: column;
}

.close{
  width: 100%;
  height: 100%;
}

.fenmain{
  background-color: #F4F7F9;
  padding: 40rpx;
  flex-shrink: 0;
}

.title{
  font-size: 29rpx;
  color: #333333;
  font-weight: bold;
  margin-bottom: 30rpx;
}

.labels{
  display: flex;
  flex-wrap: wrap;
}

.labels > view{
  min-width: 145rpx;
  padding: 0 20rpx;
  height: 60rpx;
  text-align: center;
  line-height: 60rpx;
  font-size: 26rpx;
  color: #333333;
  background-color: #fff;
  margin-bottom: 20rpx;
  border-radius: 4rpx;
  margin-right: 18rpx;
}

.labels.price > view{
  width: 205rpx;
}

.labels > view.true{
  background-color: rgba(255, 172, 44, 0.05);
  color: #FFAC2C;
  border: 2rpx solid #FFAC2C;
}

.address{
  display: flex;
  padding: 0;
}

.left{
  width: 190rpx;
  text-align: center;
  font-size: 29rpx;
  color: #333333;
  background-color: #fff;
  padding-bottom: 50rpx;
}

.left view{
  margin-top: 50rpx;
}

.left view.true{
  color: #FFAB2C;
}

.left view:last-child{
  margin-bottom: 0;
}

.right{
  width: calc(100% - 190rpx);
  padding: 35rpx;
}

.btns{
  width: calc(100% + 80rpx);
  margin-left: -40rpx;
  display: flex;
  justify-content: space-between;
  padding: 35rpx 40rpx;
  border-top: 1rpx solid #CCCCCC;
}

.btns > view{
  text-align: center;
  height: 95rpx;
  line-height: 95rpx;
  font-size: 28rpx;
  border-radius: 10rpx;
}

.reset{
  color: #999999;
  background-color: #FAFAFA;
  width: 169rpx;
}

.ensure{
  color: #ffffff;
  background-color: #002248;
  width: 450rpx;
}

.main{
  padding: 0 25rpx 25rpx 25rpx;
  position: relative;
}

.teacher{
  margin-top: 25rpx;
}